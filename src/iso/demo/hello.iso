REC "BUFFER.INIT" JMP SEC "BUFFER.INIT.END"
REC "FILE.INIT"   JMP SEC "FILE.INIT.END"
REC "STRING.INIT" JMP SEC "STRING.INIT.END"

RUN "MAIN"
RUN "SYSTEM.EXIT"

FUN "MAIN"
	POS
	VAR "PATH" SET
	STR "hello.txt"
	
	VAR "PATH" GET
	GBL "FILE.MODE.READ" GET
	NUM 0
	RUN "FILE.OPEN"
	VAR "FILE" SET POP POP
	
	POS
	VAR "STRING"         SET
	GBL "FILE.INTERFACE" GET
	VAR "FILE"           GET
	
	VAR "STRING"        GET
	RUN "CONSOLE.PRINT" POP
	RET
