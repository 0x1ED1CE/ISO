RUN "MAIN"
RUN "PROGRAM.EXIT"

DEF "PLAYER.NEW"
	POS
	NUM 2
	ADD
	HOP
DEF "PLAYER.X"
DEF "PLAYER.Y"
	NUM 1
	ADD

FUN "MAIN"
	POS
	VAR "PLAYER_1" SET REM "Allocate player struct"
	REF "PLAYER.NEW"
	
	NUM 420
	VAR "PLAYER_1" GET
	REF "PLAYER.X" SET REM "PLAYER_1.X = 420"
	
	NUM 69
	VAR "PLAYER_1" GET
	REF "PLAYER.Y" SET REM "PLAYER_1.Y = 69"
